  
{
  "manifest_version": 2,
  "name": "GitHub Recent PRs",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
    "permissions": [
    "https://*/*",
    "http://*/*",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "browser_action": {
    "default_title": "See Recent PRs",
    "default_popup": "popup.html"
  }
}
